{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node --enable-source-maps build/server.js",
    "start:prod": "node --enable-source-maps build/server.js",
    "typeorm": "tsx node_modules/typeorm/cli.js -d src/config/data-source.ts",
    "db:migrate:gen": "pnpm typeorm migration:generate src/migrations/AutoMigration",
    "db:migrate": "pnpm typeorm migration:run",
    "db:revert": "pnpm typeorm migration:revert"
  },
